{
  "plugins": [
    "react",
    "jest"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es2017": true,
    "jest": true
  },
  "globals": {
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 2,
    "comma-dangle": 0,
    "no-return-assign": 0,
    "react/forbid-prop-types": 0,
    "no-unused-vars": 1,
    "react/require-default-props": 0,
    "prefer-default-export": 0
  },
  "parser": "babel-eslint",
  "settings": {
    "react": {
      "createClass": "createReactClass",
      "pragma": "React",
      "version": "detect"
    }
  }
}
